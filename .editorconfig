root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8

# [*.{js, jsx, ts, d.ts, tsx, html, css, cjs, json, yml, yaml}]
indent_size = 2
indent_style = space

[*.md]
trim_trailing_whitespace = false

[*.{py, spec}]
indent_size = 4
indent_style = space
